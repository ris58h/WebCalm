<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <multiHostInjector implementation="ris58h.webcalm.html.HtmlAttributeJavaScriptInjector"/>
        <multiHostInjector implementation="ris58h.webcalm.html.HtmlScriptTagJavaScriptInjector"/>

        <psi.referenceContributor
                language="JavaScript"
                implementation="ris58h.webcalm.html.IdSelectorReferenceContributor"/>
    </extensions>
    <extensions defaultExtensionNs="ris58h.webcalm">
        <javascript.relatedScriptsProcessor implementation="ris58h.webcalm.html.HtmlRelatedScriptsProcessor"/>
    </extensions>
</idea-plugin>